{
    "name": "sawan/core",
    "description": "This is the first version for my laravel core",
    "license": "MIT",
    "authors": [
        {
            "name": "AbdallahSawan",
            "email": "abdallahsawan.e@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "sawan\\core\\": "src/"
        }
    },
	"keywords": [
        "framework",
        "laravel"
    ],
    "require": {
        "php": "^7.1.3",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    
    "minimum-stability": "dev",
    "prefer-stable": true
    
}
